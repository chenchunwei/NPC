@{
    ViewBag.Title = NpcMainWebContext.CurrentUnit.Name; ;
    Layout = null;
}
@using NPC.Application.Contexts
@model NPC.Application.ManageModels.RedirectMessageModel 
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>平湖人大互动平台</title>
    <link href="@Url.Content("~/css/global.css")" type="text/css" rel="stylesheet">
    <link href="@Url.Content("~/css/js.css")" type="text/css" rel="stylesheet"/>
    <script src="@Url.Content("~/js/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/ifocus.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/nav.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ae.image.resize.min.js")" type="text/javascript"></script>
</head>
<tr>
    <td bgcolor="#FFFFFF" align="left">
        <div id="clearfloat">
        </div>
        <div id="mainContent">
            <div id="showBox">
                <div class="show_title">
                    <h1>
                        网站反馈消息</h1>
                </div>
                <div class="show_txt">
                    @Model.Message
                </div>
            </div>
            @Html.Action("RightBar", "Home")
        </div>
        <div class="clearfloat">
        </div>
    </td>
</tr>
