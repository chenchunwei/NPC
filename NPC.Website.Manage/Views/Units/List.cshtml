<script type="text/javascript">
    $("#treegrid").treegrid({
        url: '../treegrid/treegrid_data1.json',
        iconCls: 'icon-redo',
        rownumbers: true,
        treeField: 'Name',
        idField: 'id',
        fit: true,
        border: 0,
        fitColumns: true,
        pagination: true,
        pageList: [10],
        title: "&nbsp;收费员列表",

        columns: [[
                    { checkbox: true },
                    { field: 'Id', hidden: true },
                    { field: 'op', title: '操作', width: 50, align: 'center' },
                    { field: 'No', title: '收费员编号', width: 80, align: 'center' },
                    { field: 'Name', title: '姓名', width: 90, align: 'center' },
                    { field: 'Sex', title: '性别', width: 50, align: 'center' },
                    { field: 'DateOfBirth', title: '出生年月', width: 100, align: 'center' },
                    { field: 'Mobile', title: '联系方式', width: 120, align: 'center' },
                    { field: 'Device', title: '设备编号', width: 80, align: 'left' },
                    { field: 'ParkingUnit', title: '所属管理单元', width: 80, align: 'left' }, //formatter: formatParkingUnit
                    {field: 'Status', title: '状态', width: 70, align: 'left' }
                ]],
        onLoadSuccess: function () {
        }
    });
     
</script>
